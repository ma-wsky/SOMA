{
  "expo": {
    "name": "SOMA",
    "slug": "fitnessapp",
    "version": "1.0.0",
    "icon": "./assets/appIcon/SOMA.png",
    "orientation": "portrait",
    "scheme": "fitnessapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.soma.app",
      "icon": "./assets/appIcon/SOMA.png"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.soma.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/appIcon/SOMA.png",
        "backgroundColor": "#AB8FFF"
      }
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-audio",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "c2152bc1-d414-4a73-a71f-1ffb7e30a65d"
      }
    },
    "owner": "hrw-mobilecomputing"
  }
}
